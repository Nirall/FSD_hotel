mixin footer-large__inf-elem-list(title, items)
    - var classM = attributes.class !=undefined ? "footer-large__inf-elem-list " + attributes.class : "footer-large__inf-elem-list"
    div(class = classM)
        ul
            li.footer-large__inf-elem-list(style="font-family: Montserrat-Bold; text-transform: uppercase; font-size: 12px; color: #1F2041; line-height: 15px;")= title                
            each val in items
                li= val                    
