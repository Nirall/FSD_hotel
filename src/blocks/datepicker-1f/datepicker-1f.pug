mixin datepicker-1f
    .datepicker-1f
        = "ДАТЫ ПРЕБЫВАНИЯ В ОТЕЛЕ"                                 
        .datepicker-1f__wrapper-input-field
            input(class = "datepicker-here datepicker-1f__input-field" id = "inDateField" inline = "true" type="text"
            data-range="true" data-multiple-dates-separator = " - " clearButton = "true")
            span.material-icons= "expand_more"