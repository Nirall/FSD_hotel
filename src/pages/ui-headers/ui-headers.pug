extends ../../ui-template
include ../../blocks/navbar/navbar
include ../../blocks/footer-large/footer-large
include ../../blocks/footer-small/footer-small
include ../../blocks/footer-medium/footer-medium


block content
    .ui-headers
        .ui-headers__empty
        .ui-headers__navbar
            +navbar(firstElem = "bolded")
        .ui-headers__navbar.ui-headers__navbar_with-name
            +navbar()(type = "without-buttons" firstElem = "bolded")
        .ui-headers__footer-large
            +footer-large()(type = "without-border")
        .ui-headers__footer-small
            +footer-small
        .ui-headers__footer-medium
            +footer-medium()(type = "for ui-kit")
        .ui-headers__empty

